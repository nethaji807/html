  <script>
  document.getElementById('search-input').addEventListener('input', function() {
    let query = this.value;
    
    if (query.length > 0) {
        fetch(`search`)
        .then(response => response.json())
        .then(data => {
            let suggestions = document.getElementById('suggestions');
            suggestions.innerHTML = '';
            
            data.forEach(function(item) {
                let suggestion = document.createElement('div');
                suggestion.textContent = item;
                suggestions.appendChild(suggestion);
            });
        });
    }
});
document.getElementById("hamburger").addEventListener("click", function() {
  const menu = document.getElementById("hamburger-menu");
  menu.style.display = "block"; // Toggle display
});

document.getElementById("closeBtn").addEventListener("click", function() {
  document.getElementById("hamburger-menu").style.display= "none";
});
</script>

  

  <script src="/static/js/base.js"></script> 
  
<script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>
<script src="https://cdn.jsdelivr.net/particles.js/2.0.0/particles.min.js"></script>
<script src="/static/js/landing_page.js"></script>
<script>

document.addEventListener("DOMContentLoaded", function () {
  function setupSwiper(swiperClass, titleId) {
    const titleDisplay = titleId ? document.getElementById(titleId) : null;

    new Swiper(swiperClass, {
      effect: "coverflow",
      centeredSlides: true,
      slidesPerView: "auto",
      spaceBetween: 0,
      initialSlide: 1,
      coverflowEffect: {
        rotate: 0,
        stretch: 30,
        depth: 200,
        modifier: 1,
        slideShadows: false,
      },
      on: {
        init: function () {
          if (titleDisplay) {
            const activeSlide = this.slides[this.activeIndex];
            const titleText = activeSlide.querySelector(".text-center")?.textContent || "";
            titleDisplay.textContent = titleText;
          }
        },
        slideChange: function () {
          if (titleDisplay) {
            const activeSlide = this.slides[this.activeIndex];
            const titleText = activeSlide.querySelector(".text-center")?.textContent || "";
            titleDisplay.textContent = titleText;
          }
        },
      },
    });
  }

  // Initialize Men and Women Swipers
  setupSwiper(".men-swiper", "men-swiper-title");
  setupSwiper(".women-swiper", "women-swiper-title");

  // Separate setup for Design Swiper with different configurations
  new Swiper(".design-swiper", {
    effect: "coverflow",
    centeredSlides: true,
    slidesPerView: "auto",
    spaceBetween: -100, // Increase spacing for wider images
    initialSlide: 1,
    coverflowEffect: {
      rotate: 0,
      stretch: 50, // Increase stretch for a wider look
      depth: 250,
      modifier: 1,
      slideShadows: false,
    },
  });
});


</script>





</body></html>
